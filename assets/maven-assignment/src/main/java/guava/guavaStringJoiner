package guava;

public class guavaStringJoiner
{
    public static String cordJoiner(String latitude, String longitude) //Method to conjoin the cordinates to a single string by
    {                                                                  // by Mitchell Humphries
        List<String> cordinates = Arrays.asList(latitude,longitude);
        String result = Joiner.on(",").join(cordinates);
        return result;
    }
}